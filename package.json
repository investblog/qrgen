{
  "name": "qrcgen",
  "version": "1.0.0",
  "description": "QR Code Generator with share links and embed snippets. Side panel workflow, offline preview, canonical URLs.",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:edge": "wxt -b edge",
    "build": "wxt build",
    "build:edge": "wxt build -b edge",
    "build:all": "wxt build && wxt build -b edge",
    "zip": "wxt zip",
    "zip:edge": "wxt zip -b edge",
    "zip:all": "wxt zip && wxt zip -b edge",
    "typecheck": "tsc --noEmit",
    "lint": "biome check src/ wxt.config.ts",
    "lint:fix": "biome check --write src/ wxt.config.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "check": "tsc --noEmit && biome check src/ wxt.config.ts && vitest run"
  },
  "dependencies": {
    "uqr": "^0.1.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "typescript": "^5.7.0",
    "vitest": "^4.0.18",
    "wxt": "^0.19.0"
  }
}
